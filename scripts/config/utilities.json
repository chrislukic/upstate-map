{
  "geocoding": {
    "rate_limit_delay": 0.15,
    "progressive_delays": {
      "after_5": 0.3,
      "after_10": 0.5
    },
    "max_retries": 3,
    "timeout": 20
  },
  "place_api": {
    "rate_limit_delay": 0.15,
    "max_retries": 3,
    "timeout": 20
  },
  "data_processing": {
    "backup_before_changes": true,
    "validate_after_changes": true,
    "dry_run_default": false
  },
  "file_paths": {
    "data_dir": "../../public/data",
    "backup_dir": "./backups"
  },
  "validation": {
    "coordinate_bounds": {
      "lat_min": -90,
      "lat_max": 90,
      "lng_min": -180,
      "lng_max": 180
    },
    "required_fields": {
      "geocoding": ["address"],
      "place_details": ["place_id"]
    }
  }
}




